{
  "name": "hackillinois",
  "displayName": "hackillinois",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
          "command": "extension.startRecording",
          "key": "ctrl+shift+a",
          "mac": "ctrl+shift+a"
      },
      {
        "command": "extension.stopRecording",
        "key": "ctrl+shift+s",
        "mac": "ctrl+shift+s"
    }
  ],
    "commands": [
      {
        "command": "extension.startRecording",
        "title": "Start Audio Recording"
      },
      {
        "command": "extension.stopRecording",
        "title": "Stop Audio Recording"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-record-lpcm16": "^1.0.1"
  }
}
